{
  "name": "@jonxpr10/the-margin",
  "description": "Notes from the margins: explorations in LLM research, mathematics, theology, and the examined life.",
  "private": true,
  "type": "module",
  "author": "Jonathan Peter Rajan <jonxpr@gmail.com>",
  "license": "CC BY-SA 4.0",
  "homepage": "https://jonxpr10.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonxpr10/the-margin/"
  },
  "scripts": {
    "docs": "tsx quartz/bootstrap-cli.ts build --serve -d docs",
    "check": "tsc --noEmit -p config/typescript/tsconfig.json && pnpm prettier . --check --config config/prettier/.prettierrc --ignore-path config/prettier/.prettierignore",
    "format": "pnpm prettier . --write --config config/prettier/.prettierrc --ignore-path config/prettier/.prettierignore",
    "test": "node --experimental-vm-modules --no-warnings --localstorage-file=/tmp/jest-localstorage.json node_modules/jest/bin/jest.js --coverage --config config/javascript/jest.config.js",
    "dev": "tsx quartz/bootstrap-cli.ts build --serve",
    "build": "tsx quartz/bootstrap-cli.ts build",
    "preview": "tsx quartz/bootstrap-cli.ts build && pnpm serve public",
    "start": "tsx quartz/bootstrap-cli.ts build && pnpm serve public -l 8080",
    "profile": "NODE_OPTIONS='--enable-source-maps' 0x --output-dir ./prof/ ./node_modules/.bin/tsx ./quartz/bootstrap-cli.ts build --concurrency=1",
    "postinstall": "pnpm playwright install && npx puppeteer browsers install chrome && patch-package",
    "test:visual": "pnpm playwright test --config config/playwright/playwright.config.ts --grep '(lostpixel)'",
    "test:a11y": "pnpm pa11y-ci --config config/pa11y/.pa11yci --sitemap http://localhost:8080/sitemap.xml --sitemap-find https://turntrout.com --sitemap-replace http://localhost:8080",
    "generate:scss": "tsx quartz/styles/generate-variables.ts",
    "setup:python": "pip install pyupgrade autoflake isort autopep8 black ruff"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "prettier --write --config config/prettier/.prettierrc --ignore-path config/prettier/.prettierignore",
      "pnpm exec stylelint --config config/stylelint/.stylelintrc.json --fix"
    ],
    "*.json": [
      "prettier --write --config config/prettier/.prettierrc --ignore-path config/prettier/.prettierignore"
    ],
    "*.{js,jsx,ts,tsx}": [
      "pnpm eslint --fix --config config/javascript/eslint.config.js",
      "prettier --write --config config/prettier/.prettierrc --ignore-path config/prettier/.prettierignore"
    ],
    ".hooks/prepare-commit-msg": [
      "prettier --write --config config/prettier/.prettierrc --ignore-path config/prettier/.prettierignore"
    ],
    "*.fish": "fish_indent",
    "{*.sh,.hooks/commit-msg,.hooks/post-commit,.hooks/pre-commit,.hooks/pre-push}": [
      "shfmt -i 2 -w",
      "shellcheck"
    ],
    "*.py": [
      "ruff check --fix",
      "autoflake --in-place",
      "isort",
      "autopep8 --in-place",
      "black",
      "ruff check --fix"
    ],
    "!(*.vale-styles)/**/*.md": [
      "prettier --write --config config/prettier/.prettierrc --ignore-path config/prettier/.prettierignore",
      "markdownlint --config config/markdownlint/.markdownlint.jsonc --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "config": "config/javascript/commitlint.config.js"
  },
  "engines": {
    "pnpm": ">=9.0.0",
    "node": ">=18.18"
  },
  "packageManager": "pnpm@10.23.0",
  "keywords": [
    "site generator",
    "ssg",
    "digital-garden",
    "markdown",
    "blog",
    "quartz"
  ],
  "bin": {
    "quartz": "./quartz/bootstrap-cli.ts"
  },
